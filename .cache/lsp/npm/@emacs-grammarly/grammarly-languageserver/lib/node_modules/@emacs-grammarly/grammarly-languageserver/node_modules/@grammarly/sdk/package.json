{
  "private": false,
  "name": "@grammarly/sdk",
  "description": "Grammarly writing SDK (no UI)",
  "version": "2.3.17",
  "license": "Apache-2.0",
  "sideEffects": false,
  "homepage": "https://developer.grammarly.com/docs",
  "repository": {
    "type": "git",
    "url": "git://github.com/grammarly/grammarly-for-developers.git"
  },
  "main": "./lib/index.node.cjs",
  "types": "./lib/sdk.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "node": {
        "import": "./lib/index.node.mjs",
        "require": "./lib/index.node.cjs"
      },
      "default": {
        "import": "./lib/index.esm.js",
        "require": "./lib/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib/sdk.d.ts",
    "lib/sdk.beta.d.ts",
    "lib/index.esm.js",
    "lib/index.js",
    "lib/index.node.cjs",
    "lib/index.node.mjs"
  ],
  "devDependencies": {
    "@grammarly/plugin-core": "2.3.17",
    "@grammarly/plugin-reactivity": "2.3.17",
    "@grammarly/plugin-sdk": "2.3.17",
    "@microsoft/api-extractor": "^7.28.7",
    "@types/jest": "^26.0.22",
    "@types/node": "^17.0.23",
    "@types/node-fetch": "^2.6.1",
    "@types/web": "^0.0.61",
    "@types/ws": "^8.5.3",
    "domhandler": "^5.0.1",
    "htmlparser2": "^7.2.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "ws": "^8.5.0"
  },
  "scripts": {
    "test": "jest --passWithNoTests"
  }
}